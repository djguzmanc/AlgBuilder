main
{
    /* ArrayList demonstration */

    arrayList list = ArrayList( 20 );
    
    //list.setAnim( false );
    
    print list.isEmpty( );
    
    for int i = 0; i < 78; 1
    {
        if ( list.size( ) % 2 == 0 )
        {
            list.add( list.size( ) / 2, i );
        }
        else
        {
            list.add( 0, i );
        }
    }
    
    while !list.isEmpty( )
    {
        list.remove( 0 );
    }
    
    print list.isEmpty( );
}
